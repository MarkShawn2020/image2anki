   "source": [
    "img = Image.open(img_path)\n",
    "data = 255 - np.array(img)\n",
    "\n",
    "def handle_col(col):\n",
    "    return np.mean(col)\n",
    "\n",
    "cols = np.apply_along_axis(handle_col, 0, data)\n",
    "for i, col in enumerate(cols):\n",
    "    print(i, col)"
   ]
