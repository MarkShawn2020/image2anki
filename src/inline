    "from enum import Enum\n",
    "\n",
    "class HeadType(int, Enum):\n",
    "    PROBLEM = 0\n",
    "    TITLE = 1\n",
    "    OTHERS = 2\n",
    "\n",
    "class Period(TypedDict):\n",
    "    y_s: int\n",
    "    y_e: int\n",
    "    head: HeadType"
   ]
