    "def getBasePeriods(img) -> List[BasePeriod]:\n",
    "    data = 1 - np.array(img.convert('1'))\n",
    "    def handle_row(row): return row.sum(axis=0)\n",
    "    row_colors = np.apply_along_axis(handle_row, 1, data)\n",
    "    # print(row_colors.shape)\n",
