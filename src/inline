    "        if j > WHITE_THRESHOLD and is_white:\n",
    "            start = i\n",
    "            is_white = False\n",
    "        elif j <= WHITE_THRESHOLD and not is_white:\n",
    "            periods.append({\"value\": (start, i)})\n",
