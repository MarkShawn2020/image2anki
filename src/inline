   "source": [
    "from typing import TypedDict, List\n",
    "from enum import Enum\n",
    "\n",
    "class HeadType(int, Enum):\n",
    "    PROBLEM = 0\n",
    "    TITLE = 1\n",
    "    OTHERS = 2\n",
    "\n",
    "class BasePeriod(TypedDict):\n",
    "    y_s: int\n",
    "    y_e: int\n",
    "\n",
    "class Period(BasePeriod):\n",
    "    head: HeadType"
   ]
